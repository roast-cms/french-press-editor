{
  "name": "@roast-cms/french-press-editor",
  "version": "0.7.4",
  "description": "An offline-first rich text editor component.",
  "main": "dist/index.js",
  "repository": "https://github.com/roast-cms/french-press-editor.git",
  "author": "dmitrizzle <d@analog.cafe>",
  "license": "MPL-2.0",
  "private": false,
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^3.8.2",
    "jest": "^23.2.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-styled-components": "^5.0.1",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.4.1",
    "size-limit": "^0.19.0",
    "style-loader": "^0.21.0",
    "typeface-rajdhani": "^0.0.54",
    "typeface-yanone-kaffeesatz": "^0.0.54",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.3"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "prepare": "npm test && babel src --out-dir dist",
    "docs": "jsdoc2md src/*.js > docs/README.md; jsdoc2md src/components/**/*.js > docs/COMPONENTS.md; jsdoc2md src/plugins/*.js > docs/PLUGINS.md; jsdoc2md src/utils/*.js > docs/UTILS.md",
    "size": "size-limit",
    "test": "npm run size && jest"
  },
  "dependencies": {
    "@roast-cms/react-button-beans": "^1.1.3",
    "@roast-cms/react-link-filter": "*",
    "@roast-cms/react-sugar-styled": "^1.0.4",
    "is-url": "^1.2.2",
    "keycode": "^2.2.0",
    "localforage": "^1.7.1",
    "localforage-getitems": "^1.4.1",
    "lodash": "^4.17.4",
    "positions": "^1.6.1",
    "slate": "^0.34.2",
    "slate-auto-replace": "^0.9.0",
    "slate-drop-or-paste-images": "^0.8.1",
    "slate-html-serializer": "^0.6.0",
    "slate-react": "^0.13.0",
    "styled-components": "^3.2.3",
    "uuid": "^3.1.0"
  },
  "size-limit": [
    {
      "path": "dist",
      "limit": "40 KB",
      "webpack": false,
      "ignore": ["*.js.snap", "*.test.js"]
    }
  ],
  "jest": {
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/examples/",
      "/graphics/"
    ]
  }
}
